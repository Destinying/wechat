<!--pages/myself/myself.wxml-->
<view>
 <view style="background:#b90611;padding: 20rpx 0;font-size: 35rpx;text-align: center;font-weight: bold;letter-spacing:5rpx;color:#fff">
    <view>个人中心</view>
    <view style="background:#fff;height:2px;width:150rpx;margin:10rpx auto 10rpx;"></view>
  </view>
  <view style="background:#fff;width:100%;padding:20rpx 0;text-align:center;font-size:30rpx;"><text>个人设置</text></view>
  <view class="back_style name">
      <view class="mount_info">账户信息</view>
  </view>
      <view class="nickname name">
          <text>昵称：</text>
          <text>{{nickname}}</text>
      </view>
      <view class="nickname">
          <text>性别：</text>
          <text>{{sex}}</text>
      </view>
      <view class="nickname">
          <text>会员类型：</text>
          <text>{{isVip}}</text>
      </view>
      <view class="nickname">
          <text>会员日期：</text>
          <text>{{out_time}}</text>
      </view>
      <view class="nickname realname">
          <text>真实姓名：</text>
          <text>{{realname}}</text>
      </view>
      <view class="nickname">
          <text>出生年月：</text>
          <text>{{birth}}</text>
      </view>
      <view class="nickname">
          <text>所在学校：</text>
          <text>{{graduateschool}}</text>
      </view>
      <view class="nickname">
          <text>联系方式：</text>
          <text>{{mobile}}</text>
      </view>
      <view class="nickname">
          <text>联系邮箱：</text>
          <text>{{email}}</text>
      </view>
</view>
